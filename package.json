{
    "name": "danscottjones.com",
    "version": "0.0.1",
    "description": "Personal blog",
    "engines": {
        "node": "7.10.0",
        "npm": "4.2.0",
        "yarn": "0.24.6"
    },
    "scripts": {
        "clean": "rm -rf dist",
        "test": "jest",
        "dev": "NODE_ENV=development webpack-dev-server --env=dev",
        "build": "npm run clean && NODE_ENV=production webpack -p --env=base",
        "start": "npm run build && http-server dist/"
    },
    "repository": "github.com/dan-j/danscottjones.com",
    "keywords": [
        "blog",
        "react",
        "node",
        "es6",
        "contentful"
    ],
    "author": "dan-j",
    "license": "MIT",
    "dependencies": {
        "contentful": "^4.3.0",
        "http-server": "^0.10.0",
        "normalize.css": "^7.0.0",
        "prop-types": "^15.5.10",
        "react": "^15.5.4",
        "react-dom": "^15.5.4",
        "react-hot-loader": "^3.0.0-beta.7",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-core": "^6.24.1",
        "babel-loader": "^7.0.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.2",
        "dotenv": "^4.0.0",
        "jest": "^20.0.3",
        "node-sass": "^4.5.3",
        "react-test-renderer": "^15.5.4",
        "sass-loader": "^6.0.5",
        "style-loader": "^0.18.1",
        "webpack": "^2.6.0",
        "webpack-dev-server": "^2.4.5",
        "webpack-merge": "^4.1.0"
    },
    "jest": {
        "testRegex": "__tests__/.*(test|spec)\\.jsx?$"
    }
}
